{
  "private": true,
  "name": "storybook-for-react-integration-tests",
  "version": "3.1.0",
  "scripts": {
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6007",
    "storybook-ci": "storybook dev -p 6007 --ci --quiet",
    "storybook:axe": "npm run build-storybook && axe-storybook",
    "storybook:axeOnly": "axe-storybook",
    "storybook:axeServer": "axe-storybook --storybook-address http://localhost:6007"
  },
  "dependencies": {
    "@chanzuckerberg/axe-storybook-testing": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "storybook": "^7.0.2"
  }
}
